{
  "name": "@extenzo/plugin-extension",
  "version": "0.0.1",
  "description": "Rsbuild plugin: write manifest.json and remove stray HTML (internal)",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "main": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch"
  },
  "dependencies": {
    "@extenzo/core": "workspace:*"
  },
  "devDependencies": {
    "@rspack/core": "^1.0.0",
    "@rsbuild/core": "^1.6.0",
    "@rslib/core": "^0.19.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@rsbuild/core": ">=1.0.0",
    "@rspack/core": ">=1.0.0"
  }
}
