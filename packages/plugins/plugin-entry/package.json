{
  "name": "@extenzo/plugin-entry",
  "version": "0.1.0-alpha.0",
  "description": "Rsbuild plugin: multi-entry, HTML and public copy (internal)",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "main": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "test": "rstest run",
    "test:coverage": "rstest run --coverage"
  },
  "dependencies": {
    "@extenzo/core": "workspace:*"
  },
  "devDependencies": {
    "@rspack/core": "^1.0.0",
    "@rsbuild/core": "^1.6.0",
    "@rslib/core": "^0.19.0",
    "@rstest/core": "^0.8.1",
    "@rstest/coverage-istanbul": "^0.2.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@rsbuild/core": ">=1.0.0",
    "@rspack/core": ">=1.0.0"
  }
}
