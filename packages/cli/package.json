    "@rsbuild/plugin-react": "^1.2.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.47"
  },
  "devDependencies": {
    "@rslib/core": "^0.19.0",
    "@types/archiver": "^6.0.0",
